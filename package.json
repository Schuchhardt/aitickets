{
  "name": "aitickets-app",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "local": "serverless offline --aws-profile serverless-deploy-user",
    "deploy": "serverless deploy --aws-profile serverless-deploy-user",
    "remove": "serverless remove --aws-profile serverless-deploy-user",
    "test:e2e": "playwright test -c e2e/playwright.config.ts",
    "test:e2e:ui": "playwright test --ui -c e2e/playwright.config.ts",
    "test:e2e:headed": "playwright test --headed -c e2e/playwright.config.ts",
    "test:e2e:smoke": "playwright test -g @smoke -c e2e/playwright.config.ts",
    "test:e2e:noserver": "NO_SERVER=1 playwright test -c e2e/playwright.config.ts",
    "e2e": "start-server-and-test dev http://localhost:4321 test:e2e"
  },
  "dependencies": {
    "@astrojs/netlify": "^6.2.2",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/vue": "^5.0.9",
    "@fontsource/prompt": "^5.1.1",
    "@fontsource/unbounded": "^5.1.2",
    "@sentry/astro": "^10.17.0",
    "@supabase/supabase-js": "^2.39.3",
    "@tailwindcss/vite": "^4.0.8",
    "@vee-validate/zod": "^4.15.0",
    "animate.css": "^4.1.1",
    "astro": "^5.6.1",
    "dompurify": "^3.2.4",
    "form-data": "4.0.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "lodash": "^4.17.21",
    "lucide-vue-next": "^0.484.0",
    "mailgun.js": "11.1.0",
    "marked": "^15.0.7",
    "mitt": "^3.0.1",
    "openai": "^4.86.1",
    "qrcode": "^1.5.4",
    "serverless": "^4.8.0",
    "serverless-offline": "^14.4.0",
    "tailwindcss": "^4.1.3",
    "uuid": "^11.1.0",
    "vee-validate": "^4.15.0",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.5.13",
    "vue-qrcode-reader": "^5.7.1",
    "zod": "^3.24.2"
  },
